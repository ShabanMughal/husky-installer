{
  "name": "husky-installer",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "bin": {
    "husky-installer": "./bin/husky-installer.js"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "cli": "node bin/husky-installer.js",
    "prepare": "husky",
