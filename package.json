{
  "name": "husky-installer",
  "version": "1.2.0",
  "description": "Interactive CLI and web interface for setting up Husky git hooks with Prettier, ESLint, and commit conventions",
  "type": "module",
  "bin": {
    "husky-installer": "./bin/husky-installer.js"
  },
  "main": "./bin/husky-installer.js",
  "files": [
    "bin",
    "dist",
    "README.md"
  ],
  "keywords": [
    "husky",
    "git-hooks",
    "prettier",
    "eslint",
    "commit",
    "emoji",
    "conventional-commits",
    "cli",
    "interactive"
  ],
  "author": "Shaban Mughal",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ShabanMughal/husky-installer.git"
  },
  "bugs": {
    "url": "https://github.com/ShabanMughal/husky-installer/issues"
  },
  "homepage": "https://github.com/ShabanMughal/husky-installer#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "cli": "node bin/husky-installer.js",
    "prepare": "husky",
    "husky:disable": "git config core.hooksPath /dev/null",
    "husky:enable": "git config core.hooksPath .husky"
  },
  "dependencies": {
    "@inquirer/prompts": "^8.0.2",
    "@mdx-js/mdx": "^3.1.1",
    "@mdx-js/react": "^3.1.1",
    "@tailwindcss/typography": "^0.5.19",
    "chalk": "^5.6.2",
    "fumadocs-core": "^16.2.4",
    "fumadocs-ui": "^16.2.4",
    "gray-matter": "^4.0.3",
    "react-router-dom": "^7.10.1",
    "rehype-highlight": "^7.0.2",
    "rehype-slug": "^6.0.0",
    "remark-gfm": "^4.0.1",
    "remark-mdx-frontmatter": "^5.2.0"
  },
  "optionalDependencies": {
    "@tailwindcss/vite": "^4.1.17",
    "lucide-react": "^0.556.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "tailwindcss": "^4.1.17"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@mdx-js/rollup": "^3.1.1",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "prettier": "^3.7.4",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "npm:rolldown-vite@7.2.5",
    "vite-plugin-raw": "^1.0.3"
  },
  "overrides": {
    "vite": "npm:rolldown-vite@7.2.5"
  }
}
